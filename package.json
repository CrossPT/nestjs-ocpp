{
  "name": "nestjs-ocpp",
  "version": "0.0.1",
  "homepage": "https://github.com/CrossPT/nestjs-ocpp#readme",
  "description": "A Nest module for OCPP protocol",
  "author": "CrossPT",
  "license": "MIT",
  "keywords": [
    "ocpp",
    "typescript",
    "charging-station",
    "central-system",
    "nestjs"
  ],
  "main": "./dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "compile": "npx ts-node scripts/compile.ts",
    "lint": "eslint \"src/**/*.ts\" --max-warnings 0",
    "format": "prettier \"src/**/*.ts\" --write",
    "build": "rimraf dist && tsc -p tsconfig.json",
    "precommit": "lint-staged",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access private",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access private --tag next",
    "prerelease": "npm run build",
    "release": "release-it"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "barrelsby": "^2.8.1",
    "directory-tree": "^3.5.2",
    "fs": "^0.0.1-security",
    "json-schema-to-typescript": "^15.0.4",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "ts-morph": "^26.0.0",
    "uuid": "^11.1.0",
    "ws": "^8.18.2"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "cpy-cli": "^5.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.1.7",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "release-it": "^19.0.3",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CrossPT/nestjs-ocpp"
  },
  "bugs": {
    "url": "https://github.com/CrossPT/nestjs-ocpp/issues"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS"
    }
  }
}